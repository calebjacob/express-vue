{
  "name": "express-vue",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/iamceege/express-vue"
  },
  "engines": {
    "node": "16.13.1"
  },
  "private": true,
  "scripts": {
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "tsc --project ./app/backend/tsconfig.json",
    "build:frontend": "vue-tsc --noEmit && vite build",
    "husky": "husky install",
    "lint": "npm run lint:css && npm run lint:source",
    "lint:css": "stylelint '**/*.scss' --fix",
    "lint:source": "eslint --ext .js,.jsx,.ts,.vue ./app/ --fix",
    "pre-commit": "npm run lint && npm run verify",
    "production": "NODE_ENV=production npm run build && node app/backend/dist/backend/src/index.js",
    "start:backend": "TS_NODE_PROJECT='./app/backend/tsconfig.json' ts-node-dev --files -r tsconfig-paths/register ./app/backend/src/index.ts",
    "start:frontend": "vite",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "jest --projects ./jest.config.backend.js",
    "test:backend:watch": "jest --watch --projects ./jest.config.backend.js",
    "test:e2e": "cypress run",
    "test:e2e:gui": "cypress open",
    "test:frontend": "jest --projects ./jest.config.frontend.js",
    "test:frontend:debug": "node --inspect-brk node_modules/.bin/jest --runInBand --projects ./jest.config.frontend.js",
    "test:frontend:watch": "jest --watch --projects ./jest.config.frontend.js",
    "verify": "npm run verify:backend && npm run verify:frontend",
    "verify:backend": "tsc --project ./app/backend/tsconfig.json --noEmit",
    "verify:frontend": "vue-tsc --noEmit"
  },
  "dependencies": {
    "app-root-path": "^3.0.0",
    "axios": "^0.21.4",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.18.1",
    "dotenv": "^9.0.2",
    "express": "^4.17.3",
    "js-cookie": "^2.2.1",
    "sass": "^1.42.1",
    "uuid": "^8.3.2",
    "vue": "^3.2.19",
    "vue-router": "^4.0.11",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^24.9.1",
    "@types/jest-when": "^2.7.3",
    "@types/js-cookie": "^2.2.7",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^15.14.9",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "@vitejs/plugin-vue": "^1.9.2",
    "@vue/compiler-sfc": "^3.2.19",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^2.0.0-rc.15",
    "cypress": "^7.7.0",
    "eslint": "^6.7.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-vue": "^7.18.0",
    "flush-promises": "^1.0.2",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-mock-extended": "^1.0.18",
    "jest-when": "^3.4.0",
    "module-alias": "^2.2.2",
    "node-mocks-http": "^1.11.0",
    "prettier": "^2.4.1",
    "stylelint": "^14.2.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.3",
    "vite": "^2.7.10",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-tsc": "^0.3.0"
  }
}
